# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                                                  controllers.Application.index
GET        /product                                           controllers.TaskList1.productDetails(prodName, prodNum: Int)
GET        /product/:prodName/:prodNum                        controllers.TaskList1.productDetails(prodName, prodNum: Int)
GET        /productRE/$prodName<[a-z]{2}\d\d>/:prodNum        controllers.TaskList1.productDetails(prodName, prodNum: Int)
GET        /taskList1                                         controllers.TaskList1.taskList

# Prefix must match `play.assets.urlPrefix`
GET        /assets/*file                                      controllers.Assets.at(file)
GET        /versionedAssets/*file                             controllers.Assets.versioned(path="/public", file: Asset)
